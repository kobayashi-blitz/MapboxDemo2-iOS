platform :ios, '14.0'

target 'MapboxDemo2-iOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Use stable Mapbox v11 version
  pod 'MapboxMaps', '11.0.0'
  
  # Use compatible Search SDK version
  pod 'MapboxSearch', '2.0.0'
  
  # Additional dependencies with compatible versions
  pod 'MapboxDirections', '~> 2.0.0'
  pod 'MapboxCoreNavigation', '~> 2.0.0'
  pod 'MapboxNavigation', '~> 2.0.0'
end
