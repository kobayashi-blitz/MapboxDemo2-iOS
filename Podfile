platform :ios, '14.0'

target 'MapboxDemo2-iOS' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Match Android version 11.12.0
  pod 'MapboxMaps', '11.12.0'
  
  # Match Android version 2.12.0
  pod 'MapboxSearch', '2.12.0'
  
  # Additional dependencies
  pod 'MapboxDirections', '~> 2.12.0'
  pod 'MapboxCoreNavigation', '~> 2.12.0'
  pod 'MapboxNavigation', '~> 2.12.0'
end
